{
  "name": "nest_base",
  "version": "0.1.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:webpack": "nest build --webpack",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:hmr": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "command": "node dist/main.command.js",
    "command:example": "npm run command example",
    "command:seed": "npm run command seed",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:run": "npx typeorm-ts-node-esm migration:run -d ./src/data-source.ts",
    "migration:show": "npx typeorm-ts-node-esm migration:show -d ./src/data-source.ts",
    "migration:revert": "npx typeorm-ts-node-esm migration:revert -d ./src/data-source.ts",
    "migration:dist:run": "typeorm migration:run -d dist/data-source.js",
    "migration:dist:show": "typeorm migration:show -d dist/data-source.js",
    "migration:dist:revert": "typeorm migration:revert -d dist/data-source.js",
    "seed:run": "ts-node -r tsconfig-paths/register ./src/seed/run-seed.ts",
    "seed:build:run": "node dist/seed/run-seed.js",
    "mysql:create:db": "npm run build & sequelize-cli db:create",
    "mysql:create:db:debug": "npm run build & sequelize-cli db:create --debug",
    "mysql:dist:create:db": "sequelize-cli db:create --debug",
    "mysql:migrate": "npm run build & sequelize-cli db:migrate",
    "mysql:migrate:debug": "npm run build & sequelize-cli db:migrate  --debug",
    "mysql:dist:migrate": "sequelize-cli db:migrate",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:open:handles": "jest",
    "test:auth": "jest --testPathPattern \"./src/auth/\"",
    "test:auth:band": "jest --testPathPattern \"./src/auth/\" --runInBand",
    "test:cats": "jest --testPathPattern \"./src/cats/\"",
    "test:mail": "jest --testPathPattern \"./src/mail/\"",
    "test:mail:band": "jest --testPathPattern \"./src/mail/\" --runInBand",
    "test:messages": "jest --testPathPattern \"./src/messages/\"",
    "test:notifications": "jest --testPathPattern \"./src/notifications/\"",
    "test:ws": "jest --testPathPattern \"./src/ws/\"",
    "test:dashboard": "jest --testPathPattern \"./src/dashboard/\"",
    "test:posts": "jest --testPathPattern \"./src/posts/\"",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:e2e:base": "jest --config ./test/jest-e2e.json --runTestsByPath \"./test/app.e2e-spec.ts\"",
    "test:e2e:auth": "jest --config ./test/jest-e2e.json --testPathPattern \"./test/auth\"",
    "test:e2e:audio": "jest --config ./test/jest-e2e.json --testPathPattern \"./test/audio\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/apollo": "^10.1.6",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/bull": "^0.6.2",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^10.1.6",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/mongoose": "^9.2.1",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-socket.io": "^9.2.0",
    "@nestjs/sequelize": "^9.0.0",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/websockets": "^9.2.0",
    "apollo-server-core": "^3.11.1",
    "apollo-server-express": "^3.11.1",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "bull": "^4.10.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.3",
    "graphql": "^16.6.0",
    "handlebars": "^4.7.7",
    "helmet": "^6.0.0",
    "joi": "^17.6.1",
    "moment": "^2.29.4",
    "mongoose": "^6.7.1",
    "mysql2": "^2.3.3",
    "nest-commander": "^3.1.0",
    "nodemailer": "^6.8.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sequelize": "^6.28.0",
    "sequelize-typescript": "^2.1.5",
    "socket.io": "^4.5.4",
    "uuid": "^9.0.0",
    "ws": "^8.11.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.19",
    "@faker-js/faker": "^7.5.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^4.10.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.6",
    "@types/passport-jwt": "^3.0.6",
    "@types/sequelize": "^4.28.14",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "28.1.3",
    "nest-commander-schematics": "^3.0.0",
    "prettier": "^2.3.2",
    "run-script-webpack-plugin": "^0.1.1",
    "sequelize-cli": "^6.5.2",
    "socket.io-client": "^4.5.4",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4",
    "webpack": "^5.75.0",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
