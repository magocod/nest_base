{
  "name": "nest_base",
  "version": "0.1.4",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:webpack": "nest build --webpack",
    "build:doc": "npx @compodoc/compodoc -p tsconfig.compodoc.json -s",
    "build:db": "npm run db:create & npm run migration:run & npm run seed:run",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:hmr": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "command": "node dist/main.command.js",
    "command:example": "npm run command example",
    "command:seed": "npm run command seed",
    "typeorm": "typeorm-ts-node-commonjs",
    "db:create": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js db:create --synchronize no --dataSource ./src/data-source.ts",
    "migration:run": "npx typeorm-ts-node-esm migration:run -d ./src/data-source.ts",
    "migration:show": "npx typeorm-ts-node-esm migration:show -d ./src/data-source.ts",
    "migration:revert": "npx typeorm-ts-node-esm migration:revert -d ./src/data-source.ts",
    "migration:dist:run": "typeorm migration:run -d dist/data-source.js",
    "migration:dist:show": "typeorm migration:show -d dist/data-source.js",
    "migration:dist:revert": "typeorm migration:revert -d dist/data-source.js",
    "seed:run": "ts-node -r tsconfig-paths/register ./src/seed/run-seed.ts",
    "seed:build:run": "node dist/seed/run-seed.js",
    "mysql:create:db": "npm run build & sequelize-cli db:create",
    "mysql:create:db:debug": "npm run build & sequelize-cli db:create --debug",
    "mysql:create:db:dist": "sequelize-cli db:create",
    "mysql:migrate": "npm run build & sequelize-cli db:migrate",
    "mysql:migrate:debug": "npm run build & sequelize-cli db:migrate --debug",
    "mysql:migrate:dist": "sequelize-cli db:migrate",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:all": "npm run test:cov & npm run test:e2e",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:auth": "jest --testPathPattern \"./src/auth/\"",
    "test:auth:band": "jest --testPathPattern \"./src/auth/\" --runInBand",
    "test:cats": "jest --testPathPattern \"./src/cats/\"",
    "test:mail": "jest --testPathPattern \"./src/mail/\"",
    "test:mail:band": "jest --testPathPattern \"./src/mail/\" --runInBand",
    "test:messages": "jest --testPathPattern \"./src/messages/\"",
    "test:notifications": "jest --testPathPattern \"./src/notifications/\"",
    "test:ws": "jest --testPathPattern \"./src/ws/\"",
    "test:dashboard": "jest --testPathPattern \"./src/dashboard/\"",
    "test:posts": "jest --testPathPattern \"./src/posts/\"",
    "test:views": "jest --testPathPattern \"./src/views/\"",
    "test:rabbitmq": "jest --testPathPattern \"./src/rabbitmq/\"",
    "test:rabbit:service": "jest test --runTestsByPath \"./src/rabbit.service.spec.ts/\"",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:e2e:app": "jest test --config ./test/jest-e2e.json --runTestsByPath \"./test/app.e2e-spec.ts\"",
    "test:e2e:base": "jest --config ./test/jest-e2e.json --runTestsByPath \"./test/app.e2e-spec.ts\"",
    "test:e2e:auth": "jest --config ./test/jest-e2e.json --testPathPattern \"./test/auth\"",
    "test:e2e:audio": "jest --config ./test/jest-e2e.json --testPathPattern \"./test/audio\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/apollo": "^10.1.7",
    "@nestjs/axios": "^1.0.1",
    "@nestjs/bull": "^0.6.2",
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.1",
    "@nestjs/graphql": "^10.1.7",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/mongoose": "^9.2.1",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/platform-socket.io": "^9.2.1",
    "@nestjs/sequelize": "^9.0.0",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/websockets": "^9.2.1",
    "@types/amqplib": "^0.10.1",
    "amqplib": "^0.10.3",
    "apollo-server-core": "^3.11.1",
    "apollo-server-express": "^3.11.1",
    "axios": "^1.2.4",
    "bcrypt": "^5.1.0",
    "bull": "^4.10.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.0",
    "dotenv": "^16.0.3",
    "graphql": "^16.6.0",
    "handlebars": "^4.7.7",
    "helmet": "^6.0.1",
    "joi": "^17.7.0",
    "moment": "^2.29.4",
    "mongoose": "^6.0.2",
    "mysql2": "^2.2.5",
    "nest-commander": "^3.4.0",
    "nodemailer": "^6.9.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.2",
    "rxjs": "^7.8.0",
    "sequelize": "^6.28.0",
    "sequelize-typescript": "^2.1.5",
    "socket.io": "^4.5.4",
    "typeorm": "^0.3.12",
    "typeorm-extension": "^2.5.2",
    "uuid": "^9.0.0",
    "ws": "^8.12.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.19",
    "@faker-js/faker": "^7.6.0",
    "@nestjs/cli": "^9.1.9",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.2.1",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^4.10.0",
    "@types/express": "^4.17.16",
    "@types/jest": "29.2.6",
    "@types/node": "^18.11.18",
    "@types/nodemailer": "^6.4.7",
    "@types/passport-jwt": "^3.0.8",
    "@types/sequelize": "^4.28.14",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.0",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "29.4.0",
    "nest-commander-schematics": "^3.0.0",
    "npm-check-updates": "^16.6.3",
    "prettier": "^2.8.3",
    "run-script-webpack-plugin": "^0.1.1",
    "sequelize-cli": "^6.5.2",
    "socket.io-client": "^4.5.4",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.2",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
